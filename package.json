{
  "name": "simplecms",
  "version": "0.3.3",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run eslint && npm run babel && npm run sass",
    "build-prod": "npm run build && npm run minify",
    "clean": "rm -rf webapp/public/css/* & cp webapp/src/index.html webapp/public/index.html & cp -rf webapp/node_modules/slick-carousel/slick/fonts webapp/public/css/fonts & rm -rf webapp/public/js/*",
    "eslint": "eslint {bin,routes,model}/**/*.js",
    "babel": "babel webapp/src/**/**.js -o webapp/public/js/app.min.js",
    "minify": "minify webapp/public/js/app.min.js --out-file webapp/public/js/app.min.js",
    "sass": "sass webapp/src/index.scss webapp/public/css/style.css --style compressed",
    "start-back": "start node bin/init.js",
    "start-front": "cd webapp && start cmd.exe /c npm start & cd ..",
    "start": "npm run start-back && npm run start-front"
  },
  "dependencies": {
    "ansi-regex": "5.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-eslint": "^9.0.0",
    "babel-minify": "^0.5.0",
    "babel-preset-react": "^6.24.1",
    "brace-expansion": "1.1.11",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "~4.17.3",
    "express-validator": "^5.3.1",
    "helmet": "^3.16.0",
    "http-errors": "~1.7.2",
    "inherits": "2.0.3",
    "minimatch": "3.0.4",
    "minimist": "1.2.6",
    "mkdirp": "0.5.1",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
    "once": "1.4.0",
    "pbkdf2": "^3.0.17",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "random-bytes": "^1.0.0",
    "react": "^16.8.4",
    "react-cookies": "^0.1.0",
    "safe-buffer": "5.1.2",
    "strip-ansi": "5.1.0",
    "wrappy": "1.0.2",
    "yallist": "3.0.3"
  }
}
