{
  "name": "simplecms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf webapp/public/css/* & rm -rf webapp/public/js/*",
    "babel": "babel webapp/src/**.js -o webapp/public/js/app.min.js --minified",
    "build": "npm run clean && npm run eslint && npm run babel && npm run sass",
    "eslint": "eslint {bin,routes,model}/**/*.js",
    "sass": "sass webapp/src/index.scss webapp/public/css/style.css",
    "start-back": "start node bin/init.js",
    "start-front": "cd webapp && start cmd.exe /c npm start & cd ..",
    "start": "npm run start-back && npm run start-front"
  },
  "dependencies": {
    "ansi-regex": "2.1.1",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-env": "^2.4.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-react": "^6.0.0",
    "brace-expansion": "1.1.11",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "helmet": "^3.13.0",
    "http-errors": "~1.6.2",
    "inherits": "2.0.3",
    "minimatch": "3.0.4",
    "minimist": "0.0.8",
    "mkdirp": "0.5.1",
    "mongoose": "^5.2.17",
    "morgan": "~1.9.0",
    "once": "1.4.0",
    "pbkdf2": "^3.0.16",
    "prettier": "^1.14.2",
    "random-bytes": "^1.0.0",
    "safe-buffer": "5.1.1",
    "strip-ansi": "3.0.1",
    "wrappy": "1.0.2",
    "yallist": "3.0.2"
  }
}
